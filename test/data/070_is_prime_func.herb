fn is_prime(x: int) -> bool {
    var i = 2;

    while i * i <= x {
        if x % i == 0 {
            return false;
        }
        i = i + 1;
    }
    return true;
}


fn main() {
    var i = 2;

    while i < 100 {
        if is_prime(i) {
            print(i);
            print(",");
        }
        i = i + 1;
    }
}
